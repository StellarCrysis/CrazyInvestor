<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="HelloWorld" package="com.example.HelloWorld" version="1.0.0" company="My Company"/>
	<app main="HelloWorld"/>
	
	<window allow-high-dpi="true"/>
	<window fps="60"/>
	<window fps="0" if="html5"/>
	
	<source path="src"/>
	
	<haxelib name="openfl"/>
	<haxelib name="actuate"/>
	<haxelib name="feathersui"/>

	<icon path="assets/icons/icon.svg"/>

	<assets path="assets/img" rename="img" />	

	<haxeflag name="--connect" value="6000"/>
</project>